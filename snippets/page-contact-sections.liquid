{% for block in section.blocks %}
    <div {{ block.shopify_attributes }}>
      {% case block.type %}

        {% when 'hero-video' %}
          {% include 'page-block-hero-video' %}

        {% when 'image-hero' %}
          {% include 'page-block-image-hero' %}

        {% when 'contact-tel-email-chat' %}
            <div class="contact-tel-email-chat">
              <div class="page-width contact-small">
                  <div class="grid contact-us--grid">
                    <div class="grid__item medium-up--one-third" data-aos="flip-down" data-aos-duration="2500"  data-aos="row-of-3">
                      <div class="contact-us--item text-center">
                      <div class="contact-svg">
                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 40 40"><g><g><path d="M36.875 25.938V17.08L23.3 25.52a6.24 6.24 0 0 1-3.3.95 6.24 6.24 0 0 1-3.3-.95l-13.575-8.44V33.75a3.129 3.129 0 0 0 3.125 3.125h27.5a3.129 3.129 0 0 0 3.125-3.125 1.562 1.562 0 1 1 3.125 0A6.257 6.257 0 0 1 33.75 40H6.25A6.257 6.257 0 0 1 0 33.75V15.046A6.223 6.223 0 0 1 2.882 9.78l3.759-2.404V1.562C6.64.7 7.34 0 8.203 0h23.594c.863 0 1.562.7 1.562 1.562v5.822l3.761 2.408A6.222 6.222 0 0 1 40 15.055v10.883a1.563 1.563 0 0 1-3.125 0zm-3.516-14.844v4.493l3.167-1.969a3.117 3.117 0 0 0-1.09-1.195zM3.471 13.616l3.17 1.971v-4.5l-2.075 1.326c-.475.304-.847.72-1.095 1.203zm26.763 3.914V3.125H9.766V17.53l8.584 5.338a3.105 3.105 0 0 0 3.3 0zm-8.672-5.577a1.562 1.562 0 1 1-3.124 0 1.562 1.562 0 0 1 3.124 0zm5.625 0a1.562 1.562 0 1 1-3.124 0 1.562 1.562 0 0 1 3.124 0zm-11.328 0a1.562 1.562 0 1 1-3.124 0 1.562 1.562 0 0 1 3.124 0z"/></g></g></svg>
                      </div>
                      <a class="cart-link" href="mailto:{{ block.settings.cart_title_3 }}" title="{{ block.settings.cart_title_3 }}">{{ block.settings.cart_title_3 }}</a>
                      {{ block.settings.cart_text_3 }}
                      </div>
                  </div>

                  <div class="grid__item medium-up--one-third" data-aos="flip-down" data-aos-duration="500"  data-aos="row-of-3">
                      <div class="contact-us--item text-center">
                      <div class="contact-svg">
                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 40 40"><g><g><path d="M25.895 9.976a4.045 4.045 0 0 1 4.09 4.04 4.045 4.045 0 0 1-4.04 4.04 4.043 4.043 0 0 1-4.04-4.124 12.81 12.81 0 0 0-7.985 7.986 4.043 4.043 0 0 1 4.124 4.038 4.045 4.045 0 0 1-4.04 4.04c-2.28 0-4.041-1.884-4.041-3.99 0-8.934 7.264-15.976 15.932-16.03zm8.245 24.162c-7.816 7.815-20.465 7.814-28.28 0-7.816-7.816-7.815-20.464 0-28.279 7.816-7.814 20.465-7.814 28.28 0 7.816 7.816 7.815 20.465 0 28.279zM20 36.71c9.215 0 16.712-7.497 16.712-16.711 0-9.214-7.497-16.711-16.712-16.711S3.288 10.784 3.288 19.999c0 9.214 7.497 16.71 16.712 16.71z"/></g></g></svg>
                      </div>
                      <a class="cart-link" href="https://wa.me/{{ block.settings.cart_title_1 | remove: '-'| remove: '+' }}" title="{{ block.settings.cart_title_1 }}">{{ block.settings.cart_title_1 }}</a>
                      {{ block.settings.cart_text_1 }}
                      </div>
                  </div>

                  <div class="grid__item medium-up--one-third" data-aos="flip-down" data-aos-duration="1500"  data-aos="row-of-3">
                    <div class="contact-us--item text-center">
                    <div class="contact-svg">
                      <svg id="SVGDoc" width="40" height="36" xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:avocode="https://avocode.com/" viewBox="0 0 40 36"><defs></defs><g><g><title>Shape 675</title><path d="M38.21749,2.7241c0.0727,0.18668 1.78252,4.67169 1.78252,13.17823c0,8.50636 -1.70962,12.99137 -1.78242,13.17823c-0.16933,0.43434 -0.5137,0.77659 -0.94779,0.94181c-0.13979,0.05311 -3.44697,1.2787 -11.8724,1.66547l-4.29853,3.88706c-0.62475,0.56505 -1.57303,0.56487 -2.19778,0l-4.29853,-3.88706c-8.42543,-0.3866 -11.73261,-1.61219 -11.8723,-1.66547c-0.43429,-0.16522 -0.77856,-0.50747 -0.94789,-0.94181c-0.0728,-0.18686 -1.78242,-4.67169 -1.78242,-13.17823c0,-8.50654 1.70962,-12.99155 1.78252,-13.17823c0.16923,-0.43452 0.5137,-0.77659 0.94789,-0.94199c6.28813,-2.39074 28.32521,-2.36249 34.53914,0c0.43429,0.16522 0.77866,0.50747 0.94799,0.94199zM35.39186,4.66316c-7.08741,-1.82408 -23.68883,-1.82623 -30.78415,0c-1.75869,6.10149 -1.7685,16.34324 0.0004,22.47888c1.35063,0.34851 4.67995,1.03104 10.71984,1.2728c0.38302,0.0152 0.74862,0.16469 1.0336,0.42236l3.63833,3.29l3.63843,-3.29c0.28499,-0.25767 0.65058,-0.40716 1.0335,-0.42236c6.03999,-0.24176 9.36951,-0.92429 10.72014,-1.2728c1.7663,-6.12688 1.76129,-16.36863 -0.0001,-22.47888zM19.99988,14.22541c0.92165,0 1.66886,0.75084 1.66886,1.67692c0,0.92608 -0.74721,1.67674 -1.66886,1.67674c-0.92165,0 -1.66876,-0.75066 -1.66876,-1.67674c0,-0.92608 0.74711,-1.67692 1.66876,-1.67692zM11.62983,14.22541c0.92165,0 1.66876,0.75084 1.66876,1.67692c0,0.92608 -0.74711,1.67674 -1.66876,1.67674c-0.92165,0 -1.66886,-0.75066 -1.66886,-1.67674c0,-0.92608 0.74721,-1.67692 1.66886,-1.67692zM28.344,14.22541c0.92165,0 1.66876,0.75084 1.66876,1.67692c0,0.92608 -0.74711,1.67674 -1.66876,1.67674c-0.92165,0 -1.66876,-0.75066 -1.66876,-1.67674c0,-0.92608 0.74711,-1.67692 1.66876,-1.67692z" fill="#1b1d20" fill-opacity="1"></path></g></g></svg>                    </div>
                      <p class="cart-text">{{ block.settings.cart_title_2 }}</p>
                      {{ block.settings.cart_text_2 }}
                    </div>
                </div>
                  </div>
              </div>
            </div>

        {% when 'map' %}
          {% include 'page-block-map' %}

        {% when 'contact-form' %}
          {% include 'page-block-contact-form' %}

        {% when 'contact-address' %}
          <div class="contact-address">
            <div class="page-width contact-small">
              {% for i in (1..5) %}

                {% capture addr_title %}title_{{ i }}{% endcapture %}
                {% capture address %}address_{{ i }}{% endcapture %}
                {% capture hours %}hours_{{ i }}{% endcapture %}
                {% capture addr_url %}url_{{ i }}{% endcapture %}
                {% capture addr_url_2 %}url_{{ i }}_{{ i }}{% endcapture %}

                {% unless block.settings[addr_title] == blank and block.settings[address] == blank and block.settings[hours] == blank and block.settings[addr_url] ==blank and block.settings[addr_url_2] ==blank %}
                  <hr class="hr--large">
                  <div class="address-item">
                    <div class="address-item--title text-left">
                      <p>{{ block.settings[addr_title] }}</p>
                    </div>
                    <div class="address-item--description text-left">
                      <p>
                        {{ block.settings[address] }}<br>
                        <span>{{ block.settings[hours] }}</span>
                      </p>
                    </div>
                    <div class="address-item--url medium-up--text-right">
                        {%- if block.settings[addr_url] != blank -%}
                            <a href="{{ block.settings[addr_url] }}" target="_blank">{{ 'contact.address.lets_go' | t }}</a><br>
                        {%- endif -%}
                        {%- if block.settings[addr_url_2] != blank -%}
                            <a href="{{ block.settings[addr_url_2] }}" target="_blank">{{ 'contact.address.contact' | t }}</a>
                        {%- endif -%}
                     
                    </div>
                  </div>
                {% endunless %}
              {% endfor %}

              <hr class="hr--large">
            </div>
          </div>

        {% when 'contact-faq' %}
          <div class="contact-faq">
            <div class="page-width contact-small">
              <div class="contact-faq--cont">
                <div class="contact-faq--copy" data-aos="fade-up" data-aos-duration="1500">
                  <h4>{{ block.settings.title }}</h4>
                  {{ block.settings.text }}
                  <a href="{{ block.settings.url }}" class="btn">{{ block.settings.label }}</a>
                </div>
                <div class="contact-faq--image">
                  <div class="image-wrap">
                    <div
                      class="grid__image-ratio grid__image-ratio--cover grid__image-ratio--square lazyload"
                      data-bgset="{% include 'bgset', image: block.settings.image %}"
                      data-sizes="auto">
                    </div>
                    <noscript>
                      <img class="lazyloaded" src="{{ block.settings.image | img_url: '400x' }}" alt="{{ block.settings.image.alt | escape }}">
                    </noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>

      {% endcase %}
    </div>
  {% endfor %}