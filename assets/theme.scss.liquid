  .section-header {
    margin-bottom: 20px;
  }
  h2 {
    text-align: center;
    color: $colorTextBody;
    font-size: 24px;
    font-weight: 700;
    line-height: 1.333;
  }
  .btn-wrap {
    display: flex;
    width: 100%;
    justify-content: center;
  }
  .note, .errors {
    margin-bottom: 30px;
    border-radius: 6px;
    min-height: 50px;
    padding: 12px;
  }
}
/*====== Contact form animation =====*/
/* Hide the browser-specific focus styles */
.floating-label-field {
  color: $colorTextBody;
  &:focus {
    outline: 0;
    box-shadow: 0;
  }

  &::placeholder {
    color: $colorTextSecondary;
  }
}
.floating-label-wrap {
  position: relative;
}
.floating-label {
  position: absolute;
  top: 0;
  left: 0;
  user-select: none;
  z-index: 1500;
}
.floating-label-field {
  &,
  & + .floating-label {
    line-height: 1;
    font: inherit;
    padding: 1.5em;
    height: 3.2em;
  }
}
.floating-label-field {
  & + .floating-label {
    position: absolute;
    top: 0;
    left: 0;
    user-select: none;
    z-index: 1500;
  }
}
.floating-label-field--s3 + .floating-label {
  position: absolute;
  top: 20px;
  left: 1.55em;
  display: inline-block;
  width: auto;
  margin: 0;
  padding: .75em;
  transition: transform .25s, opacity .25s, padding .25s ease-in-out;
  transform-origin: 0 0;
  color: rgba(255, 255, 255, .5);
}
.floating-label-field {
  & {
    position: relative;
  }
}
.floating-label-field {
  background-color: transparent;
  color: $colorTextSecondary;
}
.floating-label-field--s3 {
  padding: 13px;
  transition: border-color .25s ease-in-out;
  color: $colorTextSecondary;
  border-radius: 6px;
  border: 1px solid $colorTextSecondary;
  background-color: transparent;
}
.floating-label-input {
  &:valid {
    &:not(:placeholder-shown) {
      border-color: rgba(#2ed573, .3);
    }
  }

  &:invalid {
    border-color: rgba(#ff4757, .3);
  }
}
.floating-label-field--s3:focus + .floating-label,
 .floating-label-field--s3:not(:placeholder-shown) + .floating-label {
  z-index: 2500;
  padding: 4px 10px;
  transform: translate(0, -33px) scale(0.9);
  color: rgba(255, 255, 255, .25);
  background-color: #fff;
  color: $colorTextBody;
  font-size: 14px;
  height: 30px;
  line-height: 18px;
  font-weight: 700;
}

/*====== FAQ page =====*/
.faq-cont {
  padding-top: 30px;
  padding-bottom: 30px;

  &.page-width {
    max-width: 940px;
  }

  &.faq-title-tabs {
    margin-top: 70px;
  }

  h3 {
    color: $colorTextBody;
    font-size: 14px;
    font-weight: 700;
    line-height: 1.72;
    text-transform: uppercase;
    letter-spacing: 0.7px;
  }
  .faq--search-cont {
    margin-top: 70px;
  }
  h4.faq-search--title {
    color: #222222;
    font-size: 24px;
    font-weight: 700;
    letter-spacing: -0.6px;
  }
  .faq-search--form {
    position: relative;
    margin: 0 auto;
    width: 100%;
    max-width: 700px;
    height: 50px;
    border-radius: 25px;
    border: 1px solid $colorTextSecondary;
    background-color: #ffffff;
    display: flex;
    justify-content: space-between;
    padding: 5px;

    input {
      border-color: transparent;
      border-radius: 25px;
    }
  }
  .place_for_search {
    width: calc(100% - 90px);
  }
  .button_for_search {
    padding: 5px;
    &::placeholder {
      color: #fff;
    }
  }
  .button_for_turn_back {
    display: none;
  }
   /* Style the tab */
  .tab {
    overflow: hidden;
    display: flex;
    justify-content: space-between;
  }

  /* Style the buttons that are used to open the tab content */
  .tab button {
    border-radius: 10px;
    border: 2px solid #d1cdcd;
    background-color: #ffffff;
    outline: none;
    cursor: pointer;
    padding: 14px 16px;
    transition: 0.3s;
    min-width: calc( 25% - 15px);
    color: $colorTextBody;
    font-size: $type_base_size;
    font-weight: 700;
    font-style: normal;
    letter-spacing: normal;
    line-height: normal;
  }
  .faq-icon {
    width: 100%;
    display: inline-block;
  }
  .tab button:hover,
  .tab button.active {
    border-color: $colorBtnPrimary;
    color: $colorBtnPrimary;

    .faq-icon {
      fill: $colorBtnPrimary;
    }
  }
  .faq-questions-cont {

  }
  .tabcontent {
    display: none;
    animation: fadeEffect 1s; /* Fading effect takes 1 second */
    margin-bottom: 50px;
    margin-left: 20px;
    border-bottom: 1px solid #d1cdcd;

    p.h2 {
      font-weight: 700;
      font-style: normal;
      letter-spacing: -1.2px;
      line-height: normal;

      @include media-query($widescreen) {
        font-size: 3em;
      }
    }
    div.rte {
      margin-bottom: 20px;
    }
  }
  /* Go from zero to full opacity */
  @keyframes fadeEffect {
    from {opacity: 0;}
    to {opacity: 1;}
  }
  .collapsible-trigger--inline {
    padding: 30px 0;
    border-top: 1px solid #d1cdcd;
    width: 100%;
    color: $colorTextBody;
    font-size: 20px;
    font-weight: 700;
    letter-spacing: normal;
    line-height: 1;
    text-align: left;

    .collapsible-trigger__icon {
      left: auto;
      right: 0;
    }
  }
  .collapsible-trigger__icon--circle {
    border: medium none;
  }

  .contact-tel-email-chat & {
    padding-bottom: 100px;
  }
  h3.contact-tel--title {
    font-size: 24px;
    font-weight: 700;
    text-align: center;
    font-style: normal;
    letter-spacing: -0.6px;
    text-transform: none;
  }
  .grid.contact-us--grid {
    margin-left: 0;
  }
  .contact-us--item {
    padding: 30px 10px 20px;
    border-radius: 10px;
    transition: all ease-in .25s;
    height: 100%;

    &:hover {

      a {
      }
    }
    p {
      color: $colorTextSecondary;
      a {
        color: $colorBtnPrimary;
        text-decoration: underline;
      }
    }
  }
  .contact-svg {
    background-color: $colorBtnPrimary;
    color: #fff;
    fill: #fff;
    padding: 15px 20px 10px;
    border-radius: 32px;
    width: 88px;
    margin: 0 auto;
    height: 72px;
  }
  a.cart-link,
  p.cart-text {
    font-size: 18px;
    font-weight: 700;
    color: $colorTextBody;
    line-height: 2.0833;
    margin-bottom: 0;
  }
  .address-item {
    display: flex;
    flex-direction: row;
    width: 100%;
  }
  .contact-address {
    .hr--large {
      border-color: #d1cdcd;
    }
  }
  .address-item--title {
    width: 25%;
    padding-right: 20px;
    p {
      padding-left: 30px;
      color: $colorTextBody;
      font-size: $type_base_size;
      font-weight: 700;
      line-height: 1.5;
      background: {
        image: url({{ "icon-address-point.svg" | asset_url }});
        repeat: no-repeat;
        position: left 4px;
        color: transparent;
        size: 12px 16px;
      }
    }
  }
  .address-item--description {
    width: 62.5%;
    padding-right: 20px;

    span {
      color: $colorTextSecondary;
      font-size: 14px;
    }
  }
  .address-item--url {
    width: 12.5%;

    a {
      color: $colorBtnPrimary;
      text-decoration: underline;
    }
  }
  @include media-query($medium) {
    .tab button {
      font-size: 14px;
      padding-left: 6px;
      padding-right: 8px;
    }
    a.cart-link,
    p.cart-text {
      font-size: 18px;
    }
    .contact-us--item {
      padding-left: 8px;
      padding-right: 8px;
    }
    .contact-us--item p {
      font-size: 14px;
    }
    .contact-faq--copy {
      padding: 20px 20px 20px;

      h4 {
        font-size: 22px;
      }
    }
    .faq-cont a.cart-link, .faq-cont p.cart-text {
      font-size: 14px;
    }
  }
  @include media-query($small) {
    .tab {
      flex-direction: column;

      button {
        padding: 8px 16px;
        margin-bottom: 7px
      }
      .faq-icon {
        width: auto;
        margin-right: 8px;
        svg {
          height: 16px;
          width: auto;
        }
      }
    }
    .address-item {
      flex-direction: column;
    }
    .address-item--title,
    .address-item--description,
    .address-item--url {
      width: 100%;
      padding-right: 20px;
      padding-left: 20px;
    }
    .contact-faq--cont {
      flex-direction: column;
    }
    .contact-faq--copy {
      padding-bottom: 60px;
    }
    .contact-faq--copy,
    .contact-faq--image {
      width: 100%;
    }
    .contact-tel-email-chat .contact-us--item {
      margin-bottom: 30px;
    }
  }
}


