$label-bottom-margin: 12px;

.variant-input-wrap {
  border: 0;
  padding: 0;
  margin: 0 0 ($gutter / 1.5);
  position: relative;

  input {
    @include visuallyHidden;
  }

  label {
    @include baseFontStack;
    position: relative;
    display: inline-block;
    font-weight: 700;
    padding: 7px 15px 7px;
    margin: 0 8px $label-bottom-margin 0;
    background-color: #f1eeee;
    box-shadow: 0 0 0 1px #f1eeee;
    border-radius: 20px;
    text-align: center;

    @if ($type_body_text_center) {
      margin: 0 4px $label-bottom-margin;
    }

    &.disabled {
      color: $colorBorder;
    }

    &.disabled:after {
      position: absolute;
      content: "";
      left: 50%;
      top: 0;
      bottom: 0;
      border-left: 1px solid;
      border-color: $colorBorder;
      transform: rotate(45deg);
    }
  }

  input[type='radio']:focus + label {
    background-color: rgba($colorTextBody, 0.05);
    box-shadow: 0 0 0 2px $colorTextBody;
  }

  input[type='radio']:checked + label {
    box-shadow: 0 0 0 2px $colorBtnPrimary;
  }

  &.button-fieldset {
    margin-bottom: 8px;

    .variant-input {
      min-width: 33%;
    }
    label {
      width: 90%;
      background-color: #f1eeee;
      box-shadow: 0 0 0 2px #f1eeee;
      border-radius: 20px;
    }
    input[type='radio']:checked + label {
      background-color: rgba($colorBody, 1);
    }
  }
  &.color-fieldset {
    label {
      padding: 8px 10px;
      border-radius: 4px;
      box-shadow: 0 0 0 2px $colorBody;

      &:before {
        border-radius: 8px;
        top: -2px;
        left: -2px;
        right: -2px;
        bottom: -2px;
      }
    }
    input[type='radio'] + label {
      box-shadow: 0 0 0 2px $colorBody;
      border-radius: 4px;
    }
    input[type='radio']:checked + label {
      box-shadow: 0 0 0 2px $colorBtnPrimary;
      border-radius: 4px;
    }
  }
}

