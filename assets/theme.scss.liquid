    will-change: opacity, transform;
    opacity: 0;
  }

  [data-aos="map-section__animation"].aos-animate .map-section__container {
    animation: $animate_sections_background_style 2.5s cubic-bezier(0.26, 0.54, 0.32, 1) 0s forwards;
  }
