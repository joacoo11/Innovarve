/*============================================================================
  Grid Breakpoints and Class Names
    - Do not change breakpoint variable names
    - Medium breakpoint is also set in theme.js.liquid and inline
    throughout some templates. Be weary of changing unless you know what you're doing.
==============================================================================*/
$grid-small: 560px;

$grid-medium: 769px;

$grid-large: 960px;

$grid-widescreen: 1050px;

$grid-hdwidescreen: 1420px;

$xsmall: 'xsmall';

$small-down: 'small-down';

$small: 'small';

$medium: 'medium';

$medium-down: 'medium-down';

$medium-up: 'medium-up';

$large: 'large';

$large-down: 'large-down';

$large-up: 'large-up';

$widescreen: 'widescreen';

$hdwidescreen-down: 'hdwidescreen-down';

$hdwidescreen: 'hdwidescreen';

$grid-breakpoint-has-widths: ($small, $medium-up, $widescreen);

$grid-breakpoint-has-utility: ($small, $medium-down, $medium-up, $widescreen);

$grid-breakpoint-has-push: ($medium-up, $widescreen);

// The `$grid-breakpoints` list is used to build our media queries.

// You can use these in the media-query mixin.

$grid-breakpoints: (
  $xsmall '(max-width: #{$grid-small - 1})',
  $small-down '(min-width: #{$grid-small}) and (max-width: #{$grid-medium - 1})',
  $small '(max-width: #{$grid-medium - 1})',
  $medium '(min-width: #{$grid-medium}) and (max-width: #{$grid-large - 1})',
  $medium-down '(max-width: #{$grid-large - 1})',
  $medium-up '(min-width: #{$grid-medium})',
  $large '(min-width: #{$grid-large}) and (max-width: #{$grid-widescreen - 1})',
  $large-down '(max-width: #{$grid-widescreen - 1})',
  $large-up '(min-width: #{$grid-large})',
  $widescreen '(min-width: #{$grid-widescreen})',
  $hdwidescreen-down '(min-width: #{$grid-widescreen}) and (max-width: #{$grid-hdwidescreen - 1})',
  $hdwidescreen '(min-width: #{$grid-hdwidescreen})'
);

