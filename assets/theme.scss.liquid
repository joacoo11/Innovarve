    opacity: 0.18;
  }
  .colors-swatches {
    width: 100%;
    max-width: 1420px;
    margin: 30px auto 20px;

    ul {
      padding: 0;
      /*display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;*/
      list-style-type: none;
      margin: 0;
      width: 100%;
    }
    li.colors-swatch {
      -webkit-flex: 1;
      -ms-flex: 1;
      flex: 1;
      box-sizing: border-box;
      position: relative;
      display: block;
      height: 50px;
      border-radius: 4px;
      margin: 5px 10px 7px;
      max-width: 140px;

      &:before {
        content: '';
        position: absolute;
        width: calc(100% + 4px);
        height: 54px;
        background: 0 0;
        border: solid 2px #fff;
        left: -4px;
        top: -4px;
        z-index: 11;
        cursor: pointer;
        box-sizing: content-box;
        border-radius: 6px;
      }

      &.slick-current {

      }
    }
  }
  .landing-slider--hero {
    .color-title {
      font-size: 48px;
      font-weight: 700;
      line-height: 1.1666;
      text-transform: uppercase;
      background-color: #fff;
    }
    .images {
      border-top: solid 4px #d7e7ed;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      color: #fff;
      position: relative;
    }
    .feature-row__item-image {
      -webkit-flex: 1;
     -ms-flex: 1;
     flex: 1;
      position: relative;
      margin-bottom: 0;
      position: relative;

      &:after {
        position: absolute;
        content: '';
        height: 100%;
        width: 100%;
        top: 0;
        left: 0;
        border-left: 4px solid;
        border-bottom: 4px solid;
        border-color: inherit;
      }

      img {
      }
      &:last-child {
        &:after {
          border-right: 4px solid;
          border-color: inherit;
        }
      }
    }
    .image-wrap img {
      opacity: 1;
    }
  }
  .copy {
    position: absolute;
    width: 80%;
    top: 30px;
    left: 30px;

    h3 {
      color: #ffffff;
      font-size: 48px;
      font-weight: 700;
      line-height: 1.1666;
      margin-bottom: 30px;
    }
    p {
      color: #ffffff;
    }
    .btn {
      border-color: #fff;
      color: #fff;
      font-size: 14px;
      font-weight: 700;
      margin-top: 30px;
      transition: all ease .25s;

      &:hover {
        border-color: $colorBtnPrimary;
        background-color: $colorBtnPrimary !important;
      }
