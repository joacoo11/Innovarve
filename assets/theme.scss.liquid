.type-banner__link {
  display: block;
  -ms-flex: 1 1 100%;
      flex: 1 1 100%;
}

.type-banner__content {
  width: 100%;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
      align-items: center;
  -ms-flex-pack: center;
      justify-content: center;
  padding: 0 10px;
