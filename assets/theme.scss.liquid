/*====== page-company-history-slider =====*/
$slider-accent-red: $colorTextRed;

.page-company-history-slider {
  background-color: $colorTextSecondary;
  min-height: 96vh;

  .company-slider--years {
    position: relative;
    margin-bottom: 80px;
    &:before {
      position: absolute;
      content: '';
      width: 100%;
      height: 1px;
      background-color: $slider-accent-red;
      top: 50%;
      left: 0;
    }
  }
}
.company-slider {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding: 120px 0;

  .company-slider--years {
    width: 100%;
  }
  .timeline-wrapper {
    width: 100%;
    display: block;
    margin: 0 auto;
  }
  .timeline-nav{
    margin: 0;
    width: 100%;
    /*display: flex;
    flex-direction: row;
    justify-content: space-around;*/

    .timeline__item{
      text-align: center;
      color: #fff;
      margin: 15px;
      position: relative;
      outline: none;
      cursor: pointer;
      list-style: none;
      padding: 38px 0;

      span {
        display: inline-block;
        height: 120px;
        max-width: 120px;
        min-width: 120px;
        border-radius: 100%;
        list-style: none;
        padding: 38px 0;
        margin: 0 auto;
        background: $slider-accent-red;
        color: #fff;
        font-size: 24px;
        font-weight: 700;
        line-height: 1.667;
        letter-spacing: -0.6px;
        opacity: 0;
      }

      &:before {
        content: "";
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%,-50%);
        width: 16px;
        height: 16px;
        border-radius: 100%;
        background-color: $slider-accent-red;
        transition: .2s;
        z-index: -1;
      }

      &:hover {
        &:before {
          box-shadow: 0 0 10px #fff;
        }
      }

      &.slick-current{
        color: #fff;
        opacity: 1;
        &:before {
          background-color: $slider-accent-red;
        }
        & span {
          opacity: 1;
        }
      }
    }
  }
  .timeline-wrapper--slides {
    position: relative;
  }
  .image-wrap {
    background: transparent;
  }

  .company-slider--image {
    width: 58%;
    float: left;
  }
  .copy {
    width: 33.3%;
    float: right;
    color: #fff;
    padding-bottom: 50px;

    h3 {
      font-size: 40px;
      font-weight: 700;
      line-height: 1.4;
      letter-spacing: -1px;
      margin-bottom: 50px;
    }
    p {
      font-size: 18px;
      font-weight: 400;
      line-height: 1.667;
      letter-spacing: -0.45px;
    }
  }

  @include media-query($large) {
    padding-top: 60px;

    .company-slider--years {
      margin-bottom: 40px;
    }
  }
  @include media-query($medium) {
    .company-slider--image {
      width: 48%;
    }
    .copy {
      width: 44.3%;

      h3 {
        font-size: 32px;
        margin-bottom: 30px;
      }
    }
  }
  @include media-query($small) {
    padding-top: 60px;

    .company-slider--years {
      margin-bottom: 40px;
    }
    .timeline-nav .timeline__item span {
      height: 90px;
      max-width: 90px;
      min-width: 90px;
      padding: 32px 0;
      font-size: 18px;
    }
    .timeline-nav .timeline__item:before {
      transform: translate(50%, -50%);
    }
    .company-slider--image {
      width: 100%;
      margin-bottom: 30px;
    }
    .copy {
      width: 100%;
      clear: both;
      padding-bottom: 60px;

      h3 {
        font-size: 32px;
        margin-bottom: 30px;
      }
    }
  }
}
.timeline-arrows {
  position: absolute;
  bottom: 23px;
  right: 33.3%;
  width: auto;
  z-index: 9;
  transform: translate(100%, 0px);
  min-width: 120px;

  .slick-prev, 
  .slick-next {
    width: 50px;
    height: 50px;
    border: 2px solid #fff;
    border-radius: 50px;
  }
  .slick-arrow {
    transition: all ease .25s;
    .icon {
      top: 0px;
      display: block;
      width: 45px;
      height: 20px;
      color: #fff;
      padding: 2px;
    }
    &.slick-disabled {
      opacity: 1;
      border-color: #3b3d40;
      color: #3b3d40;
      .icon {color: #3b3d40;}
      &:hover {
        opacity: 1;
      }
    }
    &:hover {
      opacity: .55;
    }
    &.slick-prev {
      left: 0;
    }

    &.slick-next {
      right: 0;
    }
  }
  @include media-query($small) { 
    right: 50%;
    transform: translate(50%, 0px);
  }

}

